                <profile id="4fe0-ed8d-a28a-d78d::0687-0066-8df3-b83d::ab17-7eb1-b8f1-be9c::7a0c-26f5-ed1a-f92b" name="Mag-inverter Shield" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="Wargear Item">
                  <characteristics>
                    <characteristic name="Description" typeId="4465736372697074696f6e23232344415441232323">The mag-inverter shield provides its bearer with a 5+ invulnerable sae and, in addition, they count as being armed with defensive grenades when charged.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="points" value="0.0"/>
              </costs>
            </selection>
          </selections>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
          </costs>
          <categories>
            <category id="a7b4-0663-f333-ba47" name="Infantry" entryId="f74d-4679-75a6-1252" primary="false"/>
            <category id="f7bd-d5d3-9ffa-2ded" name="Troops" entryId="54726f6f707323232344415441232323" primary="true"/>
          </categories>
