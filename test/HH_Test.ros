              <profiles>
                <profile id="7238-cd37-70ee-d126::e92d-7662-17ce-3761::a3f7-2cdf-78f2-f8bf::e158-7adf-1565-ef08" name="Tanglewire" publicationId="ca571888--pubN106502" page="101" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="Wargear Item">
                  <characteristics>
                    <characteristic name="Description" typeId="4465736372697074696f6e23232344415441232323">Dangerous Terrain. A model in cover behind tanglewire has a 6+ cover save.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
