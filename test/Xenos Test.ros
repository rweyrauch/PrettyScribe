        <selection id="a12e-822f-a02a-5119" name="Blitza-bommer" entryId="24a3-a4a4-34f7-ee58::73ba-a086-3038-c166" number="1" type="unit">
          <rules>
            <rule id="24a3-a4a4-34f7-ee58::5b0b-aea5-becf-a6a6::15ea-860d-9eb6-21c3" name="Airborne" hidden="false">
              <description>This model cannot charge, can only be charged by units that can FLY, and can only attack or be attacked in the Fight phase by units that can FLY.</description>
            </rule>
            <rule id="24a3-a4a4-34f7-ee58::cebb-21df-25c7-284f::9caa-ff00-a5bd-5f04" name="Hard to Hit" hidden="false">
