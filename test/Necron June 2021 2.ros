                <characteristic typeId="f330-5e6e-4110-0978" name="W">4</characteristic>
                <characteristic typeId="13fc-b29b-31f2-ab9f" name="A">3</characteristic>
                <characteristic typeId="00ca-f8b8-876d-b705" name="Ld">10</characteristic>
