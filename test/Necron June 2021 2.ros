                <profile typeId="0bb9-43c2-e91f-436e" typeName="Warlord Trait" hidden="false" publicationId="61c6-c8c5-pubN65537" page="64" id="b3eb-7580-13e5-3cb8::f887-ffaa-b19e-0a12::77c2-34cc-f1fb-a182::9f58-d817-7c4c-e9a1::378c-afb0-e1b5-dc3d" name="Enduring Will">
