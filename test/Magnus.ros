            <profile id="e0d3-6932-c3e8-6ba9::7a81-2211-d524-8a1f::27a8-c34a-7a70-904e" name="Death to the False Emperor" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
