                        <characteristic name="AP" typeId="f7a6-e0d8-7973-cd8d">3</characteristic>
                        <characteristic name="Type" typeId="2f86-c8b4-b3b4-3ff9">Melee, Shred, Rending (6+), Specialist Weapon</characteristic>
                      </characteristics>
                    </profile>
                  </profiles>
