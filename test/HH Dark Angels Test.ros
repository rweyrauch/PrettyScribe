                        <profile id="7089-ea3d-51ac-2dc2::1b7d-f88c-e31d-5687::a778-c26c-f27d-d527::35bf-50d9-4acb-8569::6c60-169a-4e05-7fb8" name="Calibanite Warblade" hidden="false" typeId="1a1a-e592-2849-a5c0" typeName="Weapon">
