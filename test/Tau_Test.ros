        <selection id="afeb-77bb-ce6c-013e" name="Commander in XV85 Enforcer Battlesuit" entryId="deae-cfcf-1639-7d1c::08d3-6926-ccb0-17f4" publicationId="c0bb-c0cd-pubN68738" page="93" number="1" type="model">
