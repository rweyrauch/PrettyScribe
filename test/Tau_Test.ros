              <profiles>
                <profile id="eb72-b59f-cc43-a2e9::cbb7-dde3-09e6-4482::3fc5-194f-2b05-3502::7afc-3ced-bc9a-c722" name="Dawn blade" publicationId="c0bb-c0cd-pubN68738" page="123" hidden="false" typeId="d5f97c0b-9fc9-478d-aa34-a7c414d3ea48" typeName="Weapon">
