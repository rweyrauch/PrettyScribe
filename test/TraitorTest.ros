                <profile id="6a6a-c44b-4eee-66ac::005b-3f42-c65d-7680::4aa7-16ea-64d7-3e19::d9ce-23de-9b51-0707" name="Twin-Linked Heavy Bolter" publicationId="ca571888--pubN106502" page="177" hidden="false" typeId="576561706f6e23232344415441232323" typeName="Weapon">
