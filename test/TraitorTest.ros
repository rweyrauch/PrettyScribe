        <selection id="a412-f920-58a3-b825" name="Storm Eagle Assault Gunship, Legion" entryId="f3dd9d66-8d7c-1a45-fd66-d1db7f44f9ca::bd2ad8ef-8033-0a45-7ee5-ebcf07f1f117" publicationId="ca571888--pubN82424" page="57" number="1" type="unit">
