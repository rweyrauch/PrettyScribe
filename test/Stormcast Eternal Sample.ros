              <characteristics>
                <characteristic name="Ability Details" typeId="d4dc-8e81-bc0e-b8f0">Add 1 to hit rolls for attacks made by ORDER WAR MACHINES while they are wholly within 9&quot; of one or more friendly LORD-ORDINATORS.</characteristic>
              </characteristics>
            </profile>
            <profile id="c40a-f331-f505-15bc::19a9-7caa-ddad-87fb" name="Lord-Ordinator" hidden="false" typeId="1960-ca8e-67ce-2014" typeName="Unit">
