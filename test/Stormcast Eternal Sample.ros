          <profiles>
            <profile id="5048-b161-6f3d-8c30::6c27-df50-5164-0c3d" name="Lord-Relictor" hidden="false" typeId="1960-ca8e-67ce-2014" typeName="Unit">
              <characteristics>
                <characteristic name="Move" typeId="8655-6213-2824-1752">4&quot;</characteristic>
                <characteristic name="Wounds" typeId="cd0e-fea6-411f-904d">5</characteristic>
                <characteristic name="Bravery" typeId="0c85-bf79-836b-759e">9</characteristic>
