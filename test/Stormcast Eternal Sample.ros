<roster id="b0b9-93d2-e61a-b997" name="Stormcast Eternal Sample" battleScribeVersion="2.03" gameSystemId="e51d-b1a3-75fc-dc33" gameSystemName="Age of Sigmar" gameSystemRevision="55" xmlns="http://www.battlescribe.net/schema/rosterSchema">
