            <selection id="88b2-9b06-c497-b17d" name="Stormsmite Greatmace (Sequitor-Prime)" entryId="87b6-f5af-5630-028d::3172-6264-e86e-248b" number="1" type="upgrade">
              <profiles>
                <profile id="87b6-f5af-5630-028d::7b65-c24f-872d-30f1" name="Stormsmite Greatmace" hidden="false" typeId="96df-ab28-5d72-bbb3" typeName="Weapon">
                  <characteristics>
                    <characteristic name="Type" typeId="655c-362e-a663-3e50">Melee</characteristic>
                    <characteristic name="Range" typeId="ee32-7f8e-ccd7-b7b0">1&quot;</characteristic>
