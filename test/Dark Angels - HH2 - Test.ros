                      <profiles>
                        <profile id="d4ac-24af-d933-2f2c::60e2-167c-bb6e-450f::be87-e1df-f69c-27d2::c0d3-c136-ef6e-3ff7" name="Bolt Pistol" publicationId="a716-c1c4-7b26-8424" page="130" hidden="false" typeId="1a1a-e592-2849-a5c0" typeName="Weapon">
