                    <profile id="5134-76e5-8746-0fef::efc2-ecf3-e9fc-d219::23bd-0d0d-61a8-52ff" name="Combi-bolter" publicationId="a716-c1c4-7b26-8424" page="130" hidden="false" typeId="1a1a-e592-2849-a5c0" typeName="Weapon">
