                    <profile id="1431-cf29-f3a6-9a24::0e8a-1271-0c1e-59ac::e1ae-e6f7-8661-d80b::00a9-04d4-17d3-3442" name="Lightning Claw" publicationId="a716-c1c4-7b26-8424" page="137" hidden="false" typeId="1a1a-e592-2849-a5c0" typeName="Weapon">
