            <selection id="3723-5999-5329-f83f" name="Psychic Discipline: Biomancy" entryId="d5b2-f823-d621-330f::ef5f-4571-be9d-968e::7776-62fd-c6b3-0d95::0d2d-c811-8bdc-d53f::861c-3744-c4ff-ef6c" entryGroupId="d5b2-f823-d621-330f::ef5f-4571-be9d-968e::7776-62fd-c6b3-0d95::d632-a8aa-19f1-8e72" publicationId="e77a-823a-da94-16b9" page="323" number="1" type="upgrade">
