                    <profile id="1431-cf29-f3a6-9a24::1c25-a0ce-c6f7-146c::b361-3d8b-c8b6-9969::e147-61b4-fee1-d9c2" name="Chainfist" publicationId="a716-c1c4-7b26-8424" page="136" hidden="false" typeId="1a1a-e592-2849-a5c0" typeName="Weapon">
