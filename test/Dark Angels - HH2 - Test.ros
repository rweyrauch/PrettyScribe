                <profile id="4802-c63f-0d36-6024::d937-e0ce-7e8a-1d6e::510f-99ff-c46c-669b" name="Rapid-Fire Battle Cannon" publicationId="bde1-6db1-163b-3b76" page="113" hidden="false" typeId="1a1a-e592-2849-a5c0" typeName="Weapon">
