            <profile id="4095-992e-ecb5-f3e7::9590-acdc-1b4e-0963" name="Summon Chronomantic Cogs" hidden="false" typeId="2e81-5e22-c6e1-73cb" typeName="Spell">
              <characteristics>
                <characteristic name="Casting Value" typeId="2508-b604-1258-a920">6</characteristic>
