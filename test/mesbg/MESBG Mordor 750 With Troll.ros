                    <characteristic typeId="67f5-8c5e-868c-5bd5" name="Fate">1</characteristic>
                    <characteristic typeId="fc29-34ba-8883-0379" name="Heroic Actions">Channelling</characteristic>
                    <characteristic typeId="52a2-dbbe-8d97-a8a2" name="Keywords">Orc Mordor Infantry Hero</characteristic>
                    <characteristic typeId="2b79-2cec-28e7-0b0d" name="Heroic Tier">Hero of Fortitude</characteristic>
                  </characteristics>
                  <modifiers />
