                        <profile typeId="c6d6-90e7-ef64-8355" typeName="Wargear" hidden="false" publicationId="3e16-9abf-pubN102460" page="91" id="5d79-c6ed-c9a1-4bbc::3f06-c9bf-8bbb-21ea::8725-d3df-5ac3-fa5e::9610-b516-f771-251d" name="Armour">
