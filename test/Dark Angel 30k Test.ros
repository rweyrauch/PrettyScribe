                <profile id="3c85-e70e-8a08-955a::54b0-fe0f-1182-c1de::8f54-160c-7b8f-7505::79c7-90f6-b453-e799" name="Extra Armour" publicationId="ca571888--pubN105292" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="Wargear Item">
                  <characteristics>
                    <characteristic name="Description" typeId="4465736372697074696f6e23232344415441232323">Vehicles equipped with extra armour count Crew Stunned results from the Vehicle Damage table as Crew Shaken results instead.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <selections>
