            <profile id="b966-2af0-d04d-5adb::d7b2-0bd0-fdd2-b44c" name="Seeker Chariot" page="" hidden="false" typeId="800f-21d0-4387-c943" typeName="Unit">
              <characteristics>
                <characteristic name="M" typeId="0bdf-a96e-9e38-7779">12&quot;</characteristic>
